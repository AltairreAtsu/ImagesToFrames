<UserControl x:Class="ImagesToFrames.FrameNameControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ImagesToFrames"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="280">
	<DockPanel>
		<Label x:Name="FrameNameLabel" Content="1:" VerticalAlignment="Center"/>
		<TextBox x:Name="FrameNameField" MinWidth="100" MaxLines="1" TextWrapping="NoWrap"/>
		<Label Content="Alias:"/>
		<TextBox x:Name="AliasField" MaxLines="1" TextWrapping="NoWrap" Margin="0,0,5,0"/>
	</DockPanel>
</UserControl>
